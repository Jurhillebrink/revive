{
    "name": "michielgerritsen/revive",
    "description": "Revive your project by adding integration tests to your Magento 2 store",
    "require": {
        "symfony/console": "^4.3",
        "illuminate/container": "^5.8",
        "symfony/process": "^4.3",
        "mikey179/vfsstream": "^1.6",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "authors": [
        {
            "name": "Michiel Gerritsen",
            "email": "michiel@controlaltdelete.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "MichielGerritsen\\Revive\\": "src"
        },
        "files": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "MichielGerritsen\\Revive\\Test\\": "tests"
        }
    }
}
